CC = gcc
CFLAGS = -Wall -Wextra -Iinclude

SRC = main.c src/lexer.c src/parser.c src/calculator.c
OUT = calculator

all:
	$(CC) $(CFLAGS) $(SRC) -o $(OUT)

clean:
	rm -f $(OUT)
